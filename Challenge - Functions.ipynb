   "source": [
    "def most_frequent(str):\n",
    "    freq=dict()\n",
    "    for a in str:\n",
    "        b=a.casefold()\n",
    "        if b in freq:\n",
    "            freq[b]+=1\n",
    "        else:\n",
    "            freq[b]=1\n",
    "    print(freq)\n",
    "ch=input(\"Enter a word: \")\n",
    "most_frequent(ch)"
   ]
